<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="443.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <children>
      <TextArea fx:id="myTextArea" layoutX="25.0" layoutY="36.0" prefHeight="147.0" prefWidth="360.0" wrapText="true">
         <font>
            <Font size="35.0" />
         </font>
      </TextArea>
      <TextField fx:id="myTextField" layoutX="25.0" layoutY="249.0" prefHeight="44.0" prefWidth="193.0" />
      <Button fx:id="enterButton" layoutX="248.0" layoutY="252.0" mnemonicParsing="false" onAction="#submit" prefHeight="35.0" prefWidth="85.0" text="ENTER" />
      <Button fx:id="chatButton" layoutX="352.0" layoutY="249.0" mnemonicParsing="false" onAction="#switchScene" prefHeight="35.0" prefWidth="66.0" text="CHAT" />
   </children>
</AnchorPane>
